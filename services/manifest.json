{
  "name": "backend-course",
  "vendor": "vtex",
  "version": "0.0.2",
  "title": "Backend Course",
  "description": "Reference app for the Backend Course",
  "builders": {
    "node": "6.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "OMSViewer"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
